<div class="page-header">
  <h1>Dish #<%= @dish.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @dish.name %></dd>

  <dt>Cuisine</dt>
  <dd><%= @dish.cuisine.name %></dd>

  <dt>Favorites</dt>
  <dd>
    <ul>
      <% @dish.favorites.each do |favorite| %>
        <li>
          <%= favorite.user.username %>
          <i class="fa fa-heart"></i>
          <%= favorite.venue.name %>
        </li>
      <% end %>
    </ul>
  </dd>
</dl>





